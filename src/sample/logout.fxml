<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="500.0" style="-fx-background-color: #505050;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LogoutController">
   <children>
      <Label layoutX="108.0" layoutY="88.0" text="Do you really want to log out?" textFill="WHITE">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Button fx:id="yesButton" layoutX="108.0" layoutY="188.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="70.0" text="Yes" textFill="#3c3c3c">
         <font>
            <Font name="System Bold" size="15.0" />
         </font></Button>
      <Button fx:id="noButton" layoutX="323.0" layoutY="187.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="70.0" text="No" textFill="#3c3c3c">
         <font>
            <Font name="System Bold" size="17.0" />
         </font></Button>
   </children>
</AnchorPane>
